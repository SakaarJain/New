
<input type="textarea" name="textfield" id="solution0-crawl">
<a  id="solution0" href="javascript:void(0)">Crawl </a>
<div class="user-dashboard">
    <div class="row" id="site_panel"><div class="col-md-10">
    <div class="panel panel-default panel-table">
        <div class="panel-heading">
            <div class="row">
                <div class="col col-xs-12">
                    <h3 class="panel-title">
                    <a class="btn btn-primary pull-left newusergroup"  href="/new_user_groups">Create User Group</a>
                    </h3>
                </div>
            </div>
        </div>

        <div class="panel-body user_grp_panel">
            <div id="DataTables_Table_4_wrapper" class="dataTables_wrapper no-footer"><div class="dt-buttons"><a class="dt-button buttons-excel buttons-html5" tabindex="0" aria-controls="DataTables_Table_4" href="#" title="Download as Excel"><span><i class="fa fa-file-excel-o"></i></span></a><a class="dt-button buttons-pdf buttons-html5" tabindex="0" aria-controls="DataTables_Table_4" href="#" title="Download as PDF"><span><i class="fa fa-file-pdf-o"></i></span></a></div><div id="DataTables_Table_4_filter" class="dataTables_filter btn btn-primary"><label>Search:<input type="search" class="" placeholder="" aria-controls="DataTables_Table_4"></label></div><table class="table table-striped table-bordered table-list dataTable no-footer dtr-inline" id="DataTables_Table_4" role="grid" aria-describedby="DataTables_Table_4_info" style="width: 1164px;">
                <thead>
                    <tr role="row"><th class="sorting" tabindex="0" aria-controls="DataTables_Table_4" rowspan="1" colspan="1" aria-label="Group Name: activate to sort column ascending" style="width: 546px;">Group Name</th><th class="sorting" tabindex="0" aria-controls="DataTables_Table_4" rowspan="1" colspan="1" aria-label="Users: activate to sort column ascending" style="width: 311px;">Users</th><th class="sorting_disabled" rowspan="1" colspan="1" aria-label="" style="width: 195px;"><em class="fa fa-cog"></em></th></tr>
                </thead>

                <tbody>
                  {{#each userGrp}}
                <tr role="row" class="odd">
                                <td tabindex="0">{{{this.name}}}</td>
                                <td>{{{this.username}}}</td>
                                <td align="center">
                                    <a class="btn btn-default"  href="/user_groups_edit"><em class="fa fa-pencil"></em></a>
                                  <a class="btn btn-danger btn-delete-user-group"  data-value='{{{this.name}}}'   href="javascript:void(0)" ><em class="fa fa-trash"></em></a>

                                </td>
                            </tr>
                            {{/each}}
                            </tbody>
            </table><div class="dataTables_info" id="DataTables_Table_4_info" role="status" aria-live="polite">Showing 1 to 3 of 3 entries</div><div class="dataTables_paginate paging_simple_numbers" id="DataTables_Table_4_paginate"><a class="paginate_button previous disabled" aria-controls="DataTables_Table_4" data-dt-idx="0" tabindex="0" id="DataTables_Table_4_previous">Previous</a><span><a class="paginate_button current" aria-controls="DataTables_Table_4" data-dt-idx="1" tabindex="0">1</a></span><a class="paginate_button next disabled" aria-controls="DataTables_Table_4" data-dt-idx="2" tabindex="0" id="DataTables_Table_4_next">Next</a></div></div>
        </div>
    </div>
</div>
</div>
